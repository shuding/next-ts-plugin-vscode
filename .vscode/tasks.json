{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "npm",
            "script": "esbuild:watch",
            "problemMatcher": "$esbuild-watch",
            "isBackground": true,
            "group": {
                "kind": "build",
                "isDefault": true
